<h1>Lista de Videojuegos</h1>

<ul>
  <% if (videojuegos.length > 0) { %>
    <% videojuegos.forEach(v => { %>
      <li>
        <%= v.nombre %> - <%= v.plataforma %>
        [<a href="/videojuegos/<%= v.id %>">Editar</a>]
      </li>
    <% }) %>
  <% } else { %>
    <li>No hay videojuegos</li>
  <% } %>
</ul>

